#include<iostream>
using namespace std;

class A
{
   public:
   	int a,b;
   	void get()
   	{
   	    cout<<"\nEnter two Numbers:";
		   cin>>a>>b;	
	}
};
class B:public A
{ 
    public:
	void sum()
	{
		cout<<"\nAddition is :"<<a+b;
	}
};
int main()
{
	B obj;
	obj.get();
	obj.sum();
	return 0;
}
